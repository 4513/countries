{
    "name": "mibo/countries",
    "type": "library",
    "description": "PHP Library for ISO 3166-1 country codes",
    "keywords": [
        "PHP",
        "ISO 3166",
        "Countries",
        "Country",
        "Codes",
        "Standard",
        "ISO",
        "3166-1",
        "3166",
        "Country codes",
        "MiBo",
        "Package",
        "Library",
        "Lib"
    ],
    "authors": [
        {
            "name": "Michal Boris",
            "email": "michal.boris27@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MiBo\\Countries\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiBo\\Countries\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "league/iso3166": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "phpunit/php-invoker": "^5.0",
        "phpstan/phpstan": "^1.5",
        "phpstan/phpstan-strict-rules": "^1.5",
        "jetbrains/phpstorm-attributes": "^1.0",
        "slevomat/coding-standard": "^8.13",
        "phpstan/extension-installer": "^1.3",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.2"
    },
    "config": {
        "platform-check": true,
        "lock": false,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
